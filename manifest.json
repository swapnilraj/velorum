{
    "manifest_version": 2,
    "name": "Velorum",
    "version": "0.1",
    "description": "Saves specified messages on Facebook messenger",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["./js/content.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}